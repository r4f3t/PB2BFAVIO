
@{
    ViewBag.Title = "Plasiyerler";
    Layout = "~/Views/Shared/LTEMASTER.cshtml";
}




<div id="LBLProgress" class="col-lg-12 text-center" style="display:none;font-size:15pt;background:#AB1313;color:#fff;">Yükleniyor...</div>
<div id="partialTable">

</div>
<script>
    window.onload=function UrunGetir() {
          
             // alert(hareket);
              var url = "@Url.Action("PlasiyerlistPartial", "Genel")";
              $("#LBLProgress").show();
              $("#partialTable").load(url, function () {
                  $("#LBLProgress").hide();
              });
          }
</script>

